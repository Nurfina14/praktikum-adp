       cprint(" "*1, "white", "on_white",end="")
       cprint(" "*2, "white", "on_light_magenta",end="")
       cprint(" "*1, "white", "on_white")
for i in range(3):
       print(" "*2, end=" ")
       cprint(" "*16, "white", "on_light_magenta")
