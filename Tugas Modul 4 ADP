#include<iostream>
using namespace std;

int main()
{
	int x = 1, jumlah_DOR = 0;
	cout<<"Nama: Nur Fina"<<"\nNIM: 2310432016"<<"\nShift: 4"<<endl;
	cout<<"=============================================================================="<<endl;
	for (int i = 1; i<= 10; i++)
	{
		for (int j = 1; j<= 10; j++)
		{
		    if (x % 3 == 0 || x % 5 == 0)
			{ 
				jumlah_DOR++;
				cout<< "DOR" ;
			}
			else	    
			{
				cout<< x ;
	      	}
	      	cout<<"\t";
	      	x++;
		}
		cout<<endl;
		
	}
	cout<<"=============================================================================="<<endl;
	cout<<endl;
	cout << "Total DOR yang muncul adalah sebanyak " << jumlah_DOR<<endl;
		
	
	return 0;

}


	
	
	
	
	
	
	
