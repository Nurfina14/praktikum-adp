from termcolor import colored, cprint
print()
for i in range(2):
       print(" "*4, end=" ")
       cprint(" "*1, "white" ,"on_yellow", end=" ")
       print(" "*3, end=" ")
       cprint(" "*1, "white", "on_yellow", end=" ")
       print(" "*2, end=" ")
       cprint(" "*1, "white", "on_yellow")
for i in range(2):
	   print(" "*3, end=" ")
